<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_"
    tools:context="com.saki.speeop_alpha_1.MainActivity">

    <ImageView
        android:id="@+id/stars1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="-1dp"
        android:alpha="0.0"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/star1_" />

    <ImageView
        android:id="@+id/stars3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="-1dp"
        android:alpha="0.1"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/star3_" />

    <ImageView
        android:id="@+id/stars2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="-1dp"
        android:alpha="0.1"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/star2_" />

    <TextView
        android:id="@+id/quotes"
        android:layout_width="307dp"
        android:layout_height="295dp"
        android:alpha="0"
        android:fontFamily="@font/annie_use_your_telescope"
        android:text="@string/quote"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FDF9F9"
        android:textSize="34sp"
        android:typeface="sans"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.653" />

    <TextView
        android:id="@+id/quotes2"
        android:layout_width="176dp"
        android:layout_height="50dp"
        android:alpha="0"
        android:fontFamily="@font/annie_use_your_telescope"
        android:text="About"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FDF9F9"
        android:textSize="34sp"
        android:typeface="sans"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.95" />

    <ImageView
        android:id="@+id/layla_icon"
        android:layout_width="216dp"
        android:layout_height="180dp"
        android:alpha="0"
        app:layout_constraintBottom_toTopOf="@+id/quotes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.75"
        app:srcCompat="@drawable/layla_icon" />

    <ImageView
        android:id="@+id/saki_button"
        android:layout_width="134dp"
        android:layout_height="128dp"
        android:layout_marginBottom="1dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/qinguecut" />

</androidx.constraintlayout.widget.ConstraintLayout>